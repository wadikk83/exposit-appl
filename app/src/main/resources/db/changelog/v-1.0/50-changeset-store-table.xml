<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="wadikk" id="50">
        <createTable tableName="store">
            <column name="id" type="java.lang.Long" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="java.lang.String"/>
            <column name="description" type="java.lang.String"/>
            <column name="article" type="java.lang.Long"/>
        </createTable>
    </changeSet>
</databaseChangeLog>